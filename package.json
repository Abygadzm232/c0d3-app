{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts --ext .tsx",
    "autofix": "eslint . --ext .ts --ext .tsx --fix && svgo public/*.svg",
    "storybook": "start-storybook -p 6006 -c .storybook -s ./public",
    "build-storybook": "build-storybook -c .storybook -s ./public",
    "test": "jest --coverage",
    "type-check": "tsc"
  },
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "bootstrap": "^4.4.1",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-unfetch": "3.0.0",
    "next": "latest",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@storybook/addon-actions": "^5.3.12",
    "@storybook/addon-viewport": "^5.3.12",
    "@storybook/react": "^5.3.8",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/styled-jsx": "^2.2.8",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "svgo": "^1.3.2",
    "typescript": "3.7.3"
  },
  "license": "ISC",
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test && npm run type-check",
      "pre-push": "npm run lint && npm run test && npm run type-check"
    }
  }
}
